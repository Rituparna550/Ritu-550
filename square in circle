#include <stdio.h>
#include <math.h>

int main()
{
    int a;
    double pi = 2.0 * acos(0.0);
    scanf("%d", &a);
    for (int i = 1; i <= a; i++)
    {
        double r;
        scanf("%lf", &r);
        double area = (2 * r * 2 * r) - (pi * r * r);
        printf("Case %d: %.2lf\n", i, area);
    }
    return 0;
}
