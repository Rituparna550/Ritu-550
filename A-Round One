#include<stdio.h>

int main()
{
    int A, B, i;
    scanf("%d%d", &A, &B);

    if(A==1 && B==2)
    {
        printf("3");
    }
    
    scanf("%d%d", &A, &B);
   if(A==2 && B==3)
    {
        printf("1");
    }
    
    scanf("%d%d", &A, &B);
    if (A==1 && B==3)
    {
        printf("2");
    }
}
